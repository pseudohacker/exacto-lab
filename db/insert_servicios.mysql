INSERT INTO
	lista_servicios (
	PK_ITEM,
	I_DESCRIPCION,
	I_CATEGORIA,
	I_TIEMPO_RESULTADO,
	I_OBSERVACIONES,
	I_CONDICION,
	I_DISPONIBLE,
	I_KEYWORD,
	I_LINK,
	I_CREATED_BY,
	I_CREATED_ON
	) 
SELECT
	PK_ITEM,
	I_DESCRIPCION,
	I_CATEGORIA,
	I_TIEMPO_RESULTADO,
	I_OBSERVACIONES,
	I_CONDICION,
	I_DISPONIBLE,
	I_KEYWORD,
	I_LINK,
	'JLH' AS I_CREATED_BY,
	now() AS I_CREATED_ON
FROM
	import_servicios
;